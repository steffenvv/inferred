{
    "transform": {
        "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
    "testRegex": "\\.tests?\\.tsx?$",
    "moduleDirectories": ["node_modules"],
    "modulePaths": ["./src"],
    "cacheDirectory": "./.jestcache",
    "transformIgnorePatterns": ["node_modules"],
    "globals": {
        "ts-jest": {
            "tsConfig": "tsconfig.json"
        }
    }
}
